{
  "name": "white-label-cms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "tailwind": "tailwindcss -i ./tailwind.css -o ./src/css/compiledTailwind.css --watch"
  },
  "dependencies": {
    "@mongodb-js/zstd": "^1.2.2",
    "@payloadcms/db-mongodb": "3.5.0",
    "@payloadcms/email-nodemailer": "3.5.0",
    "@payloadcms/live-preview-react": "3.5.0",
    "@payloadcms/next": "3.5.0",
    "@payloadcms/payload-cloud": "3.5.0",
    "@payloadcms/richtext-lexical": "3.5.0",
    "@payloadcms/storage-s3": "3.5.0",
    "@payloadcms/ui": "3.5.0",
    "@swc/core": "^1.10.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react-router": "^5.1.20",
    "aws4": "^1.13.2",
    "cross-env": "^7.0.3",
    "graphql": "^16.9.0",
    "html5-qrcode": "^2.3.8",
    "jsqr": "^1.4.0",
    "mongodb-client-encryption": "^6.1.0",
    "next": "15.0.0",
    "payload": "3.5.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "^0.33.5",
    "snappy": "^7.2.2",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "postcss": "^8.4.49",
    "sass": "^1.82.0",
    "tailwindcss": "^3.4.16",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
